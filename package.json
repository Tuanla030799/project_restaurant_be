{
  "name": "assessment",
  "version": "0.0.1",
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:doc": "npm run db:migrate && db:seed && npm run start:dev",
    "typeorm": "node --require ts-node/register ./node_modules/typeorm/cli.js --config config/ormconfig.ts",
    "migration:create": "npm run typeorm migration:create -- -d database/migrations -n",
    "db:migrate": "npm run typeorm migration:run",
    "db:seed": "npm run command seed --",
    "db:rollback": "npm run typeorm migration:revert",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/src/main.js",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "command": "node --require ts-node/register command.ts",
    "prepush": "npm run lint"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm run prepush"
    }
  },
  "dependencies": {
    "@hapi/joi": "^17.1.1",
    "@nestjs/axios": "^3.0.2",
    "@nestjs/common": "^10.3.3",
    "@nestjs/config": "^3.2.0",
    "@nestjs/core": "^10.3.3",
    "@nestjs/event-emitter": "^2.0.4",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.3.3",
    "@nestjs/platform-fastify": "^10.3.3",
    "@nestjs/platform-socket.io": "^10.3.3",
    "@nestjs/schedule": "^4.0.1",
    "@nestjs/serve-static": "^4.0.1",
    "@nestjs/swagger": "^7.3.0",
    "@nestjs/typeorm": "^10.0.2",
    "@nestjs/websockets": "^10.3.3",
    "@types/lodash": "^4.14.202",
    "@types/lodash-es": "^4.17.12",
    "@types/multer": "^1.4.11",
    "bcrypt": "^5.1.1",
    "class-transformer": "0.5.1",
    "class-validator": "^0.14.1",
    "download": "^8.0.0",
    "fs-extra": "^11.2.0",
    "google-auth-library": "^9.6.3",
    "lodash": "^4.17.21",
    "lodash-es": "^4.17.21",
    "moment": "^2.30.1",
    "multer": "^1.4.3",
    "nestjs-typeorm-paginate": "^4.0.4",
    "nodemailer": "^6.9.9",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "pg": "^8.11.3",
    "reflect-metadata": "^0.2.1",
    "rimraf": "^5.0.5",
    "rxjs": "^7.8.1",
    "slugify": "^1.6.6",
    "swagger-ui-express": "^5.0.0",
    "typeorm": "^0.2.45",
    "typeorm-adapter": "^1.6.2"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.3.2",
    "@nestjs/schematics": "^10.1.1",
    "@nestjs/testing": "^10.3.3",
    "@types/express": "^4.17.21",
    "@types/hapi__joi": "^17.1.14",
    "@types/jest": "^29.5.12",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.11.17",
    "@types/passport-jwt": "^4.0.1",
    "@types/passport-local": "^1.0.38",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "~7.0.1",
    "@typescript-eslint/parser": "~7.0.1",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "fastify-file-interceptor": "^1.0.9",
    "fastify-multipart": "^5.3.1",
    "fastify-swagger": "^5.1.1",
    "jest": "^29.7.0",
    "prettier": "^3.2.5",
    "start-server-webpack-plugin": "^2.2.5",
    "supertest": "^6.3.4",
    "ts-jest": "^29.1.2",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "~5.3.3",
    "webpack-node-externals": "^3.0.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
